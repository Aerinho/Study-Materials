
public class TestaVariaveis {

	public static void main(String[] args) {
		
		System.out.println("Hello new test!");
		
		int idade;
		idade = 18;
		
		System.out.println(idade);
		
		idade = 30 + 10;
		
		System.out.println(idade);
		
		idade = (7 * 5) + 5;
		
		System.out.println(idade);
		
		idade = 18;
		
		System.out.println("a idade e " + idade + ", parabens!");
	}
	
}
